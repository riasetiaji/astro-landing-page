---
interface Props {
	title: string;
}

const { title } = Astro.props;
import { SEO } from "astro-seo";


//Image
import { Image } from 'astro:assets';
import logo from '../images/logo/logo.png';
import facebook from '../images/logo/facebook.png';
import linkedin from '../images/logo/linkedin.png';
import instagram from '../images/logo/instagram.png';
import tiktok from '../images/logo/tiktok.png';
import twitter from '../images/logo/twitter.png';

//CSS Tambahan
import '../components/style.css';

// Google Font Optimizer
//import { GoogleFontsOptimizer } from "astro-google-fonts-optimizer";

// Social share
import { SocialShare } from "astro-social-share";

// Astroo Hero Icons
import Sparkles from 'astro-heroicons/outline/Sparkles.astro';

---


<!doctype html>
<html lang="en">
	<head>
		<!-- SEO (Search Engine Optimization) ---------------------------------------------------------------------------------------------- -->
		<SEO
      		title="Astro Jersey Sport"
    		  description="The best jersey printing manufacturer with premium quality. Any design you like and ready to send throughout Indonesia and abroad. (Produsen printing jersey terbaik dengan kualitas premium. Desain suka-suka dan siap kirim ke seluruh Indonesia hingga luar negri.)"
			  openGraph={{
    		    basic: {
    		      title: "Astro Jersey Sport",
    		      type: "website",
    		      image: logo.src,
    		    }
    		  }}
    		  twitter={{
    		    creator: "@astro_jersey_sport"
    		  }}
    		  extend={{
    		    // extending the default link tags
    		    link: [{ rel: "icon", href: logo.src }],
    		    // extending the default meta tags
    		    meta: [
    		    	// Metadata untuk Facebook
					{ property: "og:image", content: facebook.src },
      				{ property: "og:title", content: "Astro Jersey Sport Facebook" },
      				{ property: "og:description", content: "The best jersey printing manufacturer with premium quality. Any design you like and ready to send throughout Indonesia and abroad. (Produsen printing jersey terbaik dengan kualitas premium. Desain suka-suka dan siap kirim ke seluruh Indonesia hingga luar negri.)" },

					// Metadata untuk LinkedIn
					{ property: "og:image", content: linkedin.src },
      				{ property: "og:title", content: "Astro Jersey Sport LinkedIn" },
      				{ property: "og:description", content: "The best jersey printing manufacturer with premium quality. Any design you like and ready to send throughout Indonesia and abroad. (Produsen printing jersey terbaik dengan kualitas premium. Desain suka-suka dan siap kirim ke seluruh Indonesia hingga luar negri.)" },

      				// Metadata untuk Instagram
      				{ property: "og:image", content: instagram.src },
      				{ property: "og:title", content: "Astro Jersey Sport Instagram" },
      				{ property: "og:description", content: "The best jersey printing manufacturer with premium quality. Any design you like and ready to send throughout Indonesia and abroad. (Produsen printing jersey terbaik dengan kualitas premium. Desain suka-suka dan siap kirim ke seluruh Indonesia hingga luar negri.)" },

      				// Metadata untuk TikTok
      				{ property: "og:image", content: tiktok.src },
      				{ property: "og:title", content: "Astro Jersey Sport TikTok" },
      				{ property: "og:description", content: "The best jersey printing manufacturer with premium quality. Any design you like and ready to send throughout Indonesia and abroad. (Produsen printing jersey terbaik dengan kualitas premium. Desain suka-suka dan siap kirim ke seluruh Indonesia hingga luar negri.)" },
				
					// Metadata untuk Twitter
					{ property: "og:image", content: twitter.src },
      				{ property: "og:title", content: "Astro Jersey Sport Twitter" },
      				{ property: "og:description", content: "The best jersey printing manufacturer with premium quality. Any design you like and ready to send throughout Indonesia and abroad. (Produsen printing jersey terbaik dengan kualitas premium. Desain suka-suka dan siap kirim ke seluruh Indonesia hingga luar negri.)" },
				],
    		  }}
    		/>
		
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		
